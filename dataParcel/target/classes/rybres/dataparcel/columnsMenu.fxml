<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="rybres.dataparcel.ColumnsMenuController">
   <children>
      <VBox prefHeight="40.0" prefWidth="600.0">
         <children>
            <HBox prefHeight="70.0" prefWidth="300.0">
               <children>
                  <ToolBar prefHeight="40.0" prefWidth="300.0">
                    <items>
                      <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="60.0" text="Confirm" />
                    </items>
                  </ToolBar>
                  <ToolBar nodeOrientation="RIGHT_TO_LEFT" prefHeight="40.0" prefWidth="300.0">
                    <items>
                      <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="60.0" text="Cancel" />
                    </items>
                  </ToolBar>
               </children>
            </HBox>
         </children>
      </VBox>
      <HBox layoutY="40.0" prefHeight="360.0" prefWidth="600.0">
         <children>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="200.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Keep:">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin>
                  </Text>
                  <ListView prefHeight="358.0" prefWidth="190.0">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin>
                  </ListView>
               </children>
            </VBox>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="200.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Please select which columns to keep." />
                  <HBox alignment="CENTER" prefHeight="75.0" prefWidth="200.0">
                     <children>
                        <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="80.0" text="Remove &gt;&gt;" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" prefHeight="75.0" prefWidth="200.0">
                     <children>
                        <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="80.0" text="&lt;&lt; Keep" />
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="200.0">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Removed:">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin>
                  </Text>
                  <ListView prefHeight="382.0" prefWidth="190.0">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin>
                  </ListView>
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
